{
	"name": "amyz_factorio",
	"version": "0.0.1",
	"title": "Amyzonis' Factorio",
	"author": "Amyzonis",
	"dependencies": [
		"base >= 2.0.0",
		"space-age >= 2.0.0"
	],
	"description": "A set of tweaks and advanced buildings that expand the Space Age experience.",
	"factorio_version": "2.0",
  "package": {
    "git_publish_branch": "master",
    "ignore": ["**"],
    "extra": [
      { 
        "root": ".build"
      }
    ],
    "scripts": {
      "datestamp": "sh ./build-scripts/datestamp.sh",
      "prepackage": "sh ./build-scripts/prepackage.sh",
      "postpublish": "sh ./build-scripts/postpublish.sh"
    }
  }
}
